<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cmhb.module.auth.dao.BranchChangeLogDao">

    <select id="selectWhichNeedDistribute" resultType="BranchChangeLog">
        select * from auth_branch_change_log where status = 0 and success = 0
        ORDER BY id LIMIT 500
    </select>
</mapper>
